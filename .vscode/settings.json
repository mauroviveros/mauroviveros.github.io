{
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#3a424b",
    "activityBar.background": "#3a424b",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#bf407c",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#3a424b",
    "statusBar.background": "#24292e",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#3a424b",
    "statusBarItem.remoteBackground": "#24292e",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#24292e",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#24292e99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#24292e"
}
